// Test comparison operators with different types

// Numeric comparisons
print("=== Numeric comparisons ===");
print("5 < 10: " + (5 < 10));
print("10 < 5: " + (10 < 5));
print("5 <= 5: " + (5 <= 5));
print("5 >= 5: " + (5 >= 5));
print("5 == 5: " + (5 == 5));
print("5 != 10: " + (5 != 10));

// Mixed numeric types
print("=== Mixed numeric types ===");
let i: i32 = 5;
let f: f64 = 5.0;
print("i32(5) == f64(5.0): " + (i == f));
print("i32(5) < f64(5.1): " + (i < 5.1));
print("i32(5) > f64(4.9): " + (i > 4.9));

// Signed negative vs positive
let neg: i32 = -1;
let pos: i32 = 1;
print("i32(-1) < i32(1): " + (neg < pos));

// String equality
print("=== String equality ===");
print("'hello' == 'hello': " + ("hello" == "hello"));
print("'hello' != 'world': " + ("hello" != "world"));
print("'' == '': " + ("" == ""));
print("'' != 'a': " + ("" != "a"));

// Null comparisons
print("=== Null comparisons ===");
print("null == null: " + (null == null));
print("null != 0: " + (null != 0));
print("null != '': " + (null != ""));
print("null != false: " + (null != false));

// Boolean comparisons
print("=== Boolean comparisons ===");
print("true == true: " + (true == true));
print("false == false: " + (false == false));
print("true != false: " + (true != false));

print("All comparison type tests passed");
