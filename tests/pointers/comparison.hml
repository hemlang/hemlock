// Test pointer comparison operations
// Previously comparing two non-null pointers would fail

// Allocate memory for comparison tests
let p1 = alloc(64);
let p2 = alloc(64);
let p3 = p1;  // Same pointer

// Equality comparisons
print("p1 == p1: " + (p1 == p1));
print("p1 == p3: " + (p1 == p3));
print("p1 == p2: " + (p1 == p2));

// Inequality comparisons
print("p1 != p2: " + (p1 != p2));
print("p1 != p3: " + (p1 != p3));

// Null comparisons
let pnull = null;
print("p1 == null: " + (p1 == null));
print("pnull == null: " + (pnull == null));
print("p1 != null: " + (p1 != null));

// Relational comparisons (address ordering)
// Note: p1 and p2 order depends on allocator, so we test with pointer arithmetic
let base = alloc(128);
let offset1 = base + 10;
let offset2 = base + 50;

print("offset1 < offset2: " + (offset1 < offset2));
print("offset2 > offset1: " + (offset2 > offset1));
print("offset1 <= offset1: " + (offset1 <= offset1));
print("offset2 >= offset1: " + (offset2 >= offset1));

// Cleanup
free(p1);
free(p2);
free(base);
