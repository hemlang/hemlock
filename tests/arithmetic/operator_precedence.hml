// Test operator precedence

// Multiplication before addition
print("=== Mul/Add Precedence ===");
print("2 + 3 * 4 = " + (2 + 3 * 4));      // 14
print("2 * 3 + 4 = " + (2 * 3 + 4));      // 10
print("2 + 3 + 4 * 5 = " + (2 + 3 + 4 * 5)); // 25

// Division before subtraction
print("=== Div/Sub Precedence ===");
print("10 - 6 / 2 = " + (10 - 6 / 2));    // 7
print("10 / 2 - 6 = " + (10 / 2 - 6));    // -1

// Parentheses override
print("=== Parentheses ===");
print("(2 + 3) * 4 = " + ((2 + 3) * 4));  // 20
print("2 * (3 + 4) = " + (2 * (3 + 4)));  // 14
print("(10 - 6) / 2 = " + ((10 - 6) / 2)); // 2

// Comparison vs arithmetic
print("=== Comparison Precedence ===");
print("2 + 3 == 5: " + (2 + 3 == 5));     // true
print("2 * 3 < 10: " + (2 * 3 < 10));     // true
print("10 / 2 >= 5: " + (10 / 2 >= 5));   // true

// Logical vs comparison
print("=== Logical Precedence ===");
print("true && 1 < 2: " + (true && 1 < 2)); // true
print("1 < 2 && 3 < 4: " + (1 < 2 && 3 < 4)); // true
print("1 > 2 || 3 < 4: " + (1 > 2 || 3 < 4)); // true

// Unary minus
print("=== Unary Precedence ===");
print("-5 + 3 = " + (-5 + 3));            // -2
print("5 + -3 = " + (5 + -3));            // 2
print("-5 * 3 = " + (-5 * 3));            // -15
print("5 * -3 = " + (5 * -3));            // -15
print("-5 * -3 = " + (-5 * -3));          // 15

// Chained comparisons (not supported - need && for multiple)
print("=== Chained with && ===");
print("1 < 2 && 2 < 3: " + (1 < 2 && 2 < 3)); // true
print("1 < 2 && 2 > 3: " + (1 < 2 && 2 > 3)); // false

// Bitwise precedence
print("=== Bitwise Precedence ===");
print("1 | 2 & 3 = " + (1 | 2 & 3));      // 1 | (2 & 3) = 1 | 2 = 3
print("(1 | 2) & 3 = " + ((1 | 2) & 3));  // 3 & 3 = 3
print("1 << 2 + 1 = " + (1 << 2 + 1));    // 1 << 3 = 8 (+ has higher precedence? or left to right)
print("(1 << 2) + 1 = " + ((1 << 2) + 1)); // 4 + 1 = 5

// Modulo precedence (same as mul/div)
print("=== Modulo Precedence ===");
print("10 + 7 % 3 = " + (10 + 7 % 3));    // 10 + 1 = 11
print("(10 + 7) % 3 = " + ((10 + 7) % 3)); // 17 % 3 = 2

print("All operator precedence tests passed");
