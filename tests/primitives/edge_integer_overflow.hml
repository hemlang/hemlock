// Test integer overflow behavior for various types
// Expected: PASS (testing overflow detection)

// i8 overflow
try {
    let x: i8 = 127;
    let y: i8 = x + 1;
    print("ERROR: i8 overflow should have been caught");
} catch (e) {
    print("i8 overflow caught: " + e);
}

// i8 underflow
try {
    let x: i8 = -128;
    let y: i8 = x - 1;
    print("ERROR: i8 underflow should have been caught");
} catch (e) {
    print("i8 underflow caught: " + e);
}

// i16 overflow
try {
    let x: i16 = 32767;
    let y: i16 = x + 1;
    print("ERROR: i16 overflow should have been caught");
} catch (e) {
    print("i16 overflow caught: " + e);
}

// i32 overflow - test if detected
try {
    let x: i32 = 2147483647;
    let y: i32 = x + 1;
    print("i32 overflow not caught, result: " + typeof(y));
} catch (e) {
    print("i32 overflow caught: " + e);
}

// u8 overflow
try {
    let x: u8 = 255;
    let y: u8 = x + 1;
    print("ERROR: u8 overflow should have been caught");
} catch (e) {
    print("u8 overflow caught: " + e);
}

// u16 overflow
try {
    let x: u16 = 65535;
    let y: u16 = x + 1;
    print("ERROR: u16 overflow should have been caught");
} catch (e) {
    print("u16 overflow caught: " + e);
}

// u32 overflow
try {
    let x: u32 = 4294967295;
    let y: u32 = x + 1;
    print("u32 overflow not caught or detected");
} catch (e) {
    print("u32 overflow caught: " + e);
}

print("PASS");
