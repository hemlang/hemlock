// Test primitive types and basic operations

// Integers
let a = 42;
print(a);

// Floats
let b = 3.14;
print(b);

// Booleans
print(true);
print(false);

// Null
print(null);

// Strings
let s = "hello";
print(s);

// Arithmetic
print(10 + 5);
print(10 - 5);
print(10 * 5);
print(10 / 5);
print(10 % 3);

// Comparisons
print(5 == 5);
print(5 != 3);
print(5 < 10);
print(5 > 3);
print(5 <= 5);
print(5 >= 5);

print("done");
