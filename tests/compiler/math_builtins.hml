// Test math builtins

// sqrt
let x = sqrt(16.0);
print(x);

let y = sqrt(25.0);
print(y);

// floor, ceil, round
print(floor(3.7));
print(ceil(3.2));
print(round(3.5));
print(round(3.4));

// abs
print(abs(-5));
print(abs(5));
print(abs(-3.14));

// min, max
print(min(10, 5));
print(max(10, 5));
print(min(3.14, 2.71));
print(max(3.14, 2.71));

// pow, exp, log
print(pow(2, 3));
print(pow(2.0, 0.5));

// trunc
print(trunc(3.9));
print(trunc(-3.9));

// Trig functions (just test they work, not exact values)
let pi = 3.14159265359;
let sinval = sin(0.0);
print(sinval);

let cosval = cos(0.0);
print(cosval);

// clamp
print(clamp(5, 0, 10));   // 5 (in range)
print(clamp(-5, 0, 10));  // 0 (below min)
print(clamp(15, 0, 10));  // 10 (above max)

// rand_range - just test it runs (seeded for reproducibility)
seed(42);
let r = rand_range(0.0, 10.0);
print(r >= 0.0 && r <= 10.0);  // true

// log10, log2
print(log10(100.0));  // 2
print(log2(8.0));     // 3

// atan2
print(atan2(1.0, 1.0) > 0.0);  // true (pi/4 radians)

print("done");
