// Test: Open file and read in chunks
let fw = open("tests/temp/test_chunks.txt", "w");
fw.write("Hello, World!");
fw.close();

let f = open("tests/temp/test_chunks.txt", "r");
print(f);
print("Mode:");
print(f.mode);
print("Closed:");
print(f.closed);
let chunk1 = f.read(5);
print(chunk1);
let chunk2 = f.read(8);
print(chunk2);
f.close();
print("After close:");
print(f.closed);
