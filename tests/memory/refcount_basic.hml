// Test: Basic reference counting - expressions are released
// This test ensures that expression results are properly released

// Standalone expression statements should not leak
"hello";
[1, 2, 3];
({ x: 10, y: 20 });  // Parentheses force object literal interpretation
42 + 58;

// Variable assignments should have correct ref_count
let s = "world";
let arr = [4, 5, 6];
let obj = { a: 1, b: 2 };

// Reassignment should release old values
s = "new string";
arr = [7, 8, 9];
obj = { c: 3, d: 4 };

// Nested expressions
let combined = "prefix_" + "suffix";
let computed = [1, 2].concat([3, 4]);

// Print to verify execution completed
print("refcount_basic test passed");
